{
  "name": "@nounbricks/traits-nouns",
  "version": "0.0.1",
  "description": "Nouns assets to be used with @nounbricks/nounbricks-vue",
  "module": "./dist/index.es.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/amk-dev/nounbricks.git"
  },
  "exports": {
    ".": {
      "import": "./dist/index.es.js"
    }
  },
  "types": "dist/types/index.d.ts",
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "build": "vite build --mode prod && tsc --declaration --declarationDir dist/types --emitDeclarationOnly",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --ext .js,.ts,.vue, --ignore-path ./../../.gitignore ./src",
    "format": "prettier --ignore-path ./../../.gitignore ./src --check"
  },
  "devDependencies": {
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "jsdom": "^20.0.0",
    "prettier": "2.7.1",
    "typescript": "^4.6.4",
    "vite": "^3.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "author": {
    "name": "Akash K"
  }
}
